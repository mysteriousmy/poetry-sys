Page{
  background-size: cover;
  background-image: url('data:image/jpeg;base64,');
}
.load{
   
   background-color:rgba(0,0,0,0.5);
   backdrop-filter: blur(10px);
   border-radius: 6px;
   width: 90%;
   box-shadow: 0 2px 4px -1px rgb(0 0 0 / 20%), 0 4px 5px 0 rgb(0 0 0 / 14%), 0 1px 10px 0 rgb(0 0 0 / 12%);
   height: 300rpx;
   /*设置图片和文字垂直居中对齐*/
   display: flex;
   flex-direction: column;
   justify-content: center;
   margin: 0 auto;
   margin-top: 40px;
   align-items: center;
   margin-bottom: 30px;
}
.load image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.load text{
  color:white;
}
.exit{
  width: 95%;
  margin-top: 40rpx;
}
.list_card{
    background-color:rgba(255,255,255,0.4);
    backdrop-filter: blur(20px);
    border-radius: 6px;
    width: 90%;
    margin: 0 auto;
    box-shadow: 0 2px 4px -1px rgb(0 0 0 / 20%), 0 4px 5px 0 rgb(0 0 0 / 14%), 0 1px 10px 0 rgb(0 0 0 / 12%);
    height: auto;
    /*设置图片和文字垂直居中对齐*/
    display: flex;
    padding-top: 40px;
    padding-bottom: 100px;
    flex-direction: column;
    margin-bottom: 20px;
}
.btn{
    background-color: transparent;
    font-style:unset;
}
.btn-hover{
    transition: 0.1s;
    width: 100%;
    color:white;
    background-color: #1296db;
}
.home_tool {
  background-color: #ffffff;
  border-radius: 6px;
  width: 90%;
  height: auto;
  margin: 0 auto;
  box-shadow: 0 2px 4px -1px rgb(0 0 0 / 20%), 0 4px 5px 0 rgb(0 0 0 / 14%), 0 1px 10px 0 rgb(0 0 0 / 12%);
  /*设置图片和文字垂直居中对齐*/
  display: flex;
  padding-top: 10px;
  padding-bottom: 30px;
  flex-direction: row;
  justify-content: space-around;
  margin-bottom: 30px;
  margin-top: 30px;
}
.tool {
  width: 80px;
  height: 80px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  text-align: center;
}

.tool image {
  width: 60px;
  height: 60px;
  display: inline;
}

.tool text {
  width: 80px;
}